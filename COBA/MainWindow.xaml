<Window x:Class="COBA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:COBA"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="TeksAja" HorizontalAlignment="Left" Height="32" Margin="270,133,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="130" TextChanged="TextBox_TextChanged"/>
        <Button x:Name="TombolAja" Content="Button" HorizontalAlignment="Left" Height="31" Margin="270,170,0,0" VerticalAlignment="Top" Width="130" Click="Button_Click"/>
        <TextBox x:Name="TeksAja2" HorizontalAlignment="Left" Height="26" Margin="270,0,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="130" TextChanged="TextBox_TextChanged_1"/>
    </Grid>
</Window>
